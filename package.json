    "search",
    "windows",
    "command",
    "spotlight",
    "search",
    "on",
    "desktop"
  ],
  "author": "Vergo D. Tran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RoboWonder/Windows-Quick/issues"
  },
  "homepage": "https://github.com/RoboWonder/Windows-Quick#readme",
  "devDependencies": {
    "@types/node": "^11.11.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.12",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^4.1.0",
    "electron-builder": "^20.39.0",
    "electron-rebuild": "^1.8.4",
    "fuse-box": "^3.7.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.3333",
    "typescript-plugin-styled-components": "^1.3.0"
  },
  "dependencies": {
    "co": "^4.6.0",
    "electron-updater": "^4.0.6",
    "file-type": "^10.9.0",
