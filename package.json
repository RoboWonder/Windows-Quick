{
  "name": "windows-quick",
  "version": "1.0.0",
  "description": "Windows Quick is a tool search and command like Spotlight search on Mac OS and more",
  "main": "build/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV='dev' node fuse.js",
    "build": "node fuse.js",
    "start": "cross-env ENV='dev' electron .",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "postinstall": "electron-builder install-app-deps"
  },
